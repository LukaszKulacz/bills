{
    "builds": [
      {
        "src": "app/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" }
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/wsgi.py"
      }
    ]
  }